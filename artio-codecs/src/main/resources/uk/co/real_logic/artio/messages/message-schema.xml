        <field name="errorType" id="1" type="GatewayError"/>
        <field name="libraryId" id="2" type="LibraryId"/>
        <field name="replyToId" id="3" type="CorrelationId"/>
        <data name="message" id="4" type="Utf8String"/>
    </sbe:message>

